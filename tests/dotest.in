#!/bin/bash

cd @PROJECT_PATH@/tests
$1 $2 finaloutput=/tmp/regtest.g2
echo `cat /tmp/regtest.g2 | awk -F ' ' '{printf "%5.5g\n", $1}' | md5sum | awk -F ' ' '{print $1}'`> /tmp/regtest.md5
diff -u /tmp/regtest.md5 $3
result=$?
rm -f /tmp/regtest.g2 /tmp/regtest.md5

test $result -eq 0 && exit 0
exit 1
